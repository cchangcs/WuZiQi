    BEGIN
        MENUITEM "New Game(&N)\tCtrl+N",        ID_FILE_NEW1
        MENUITEM "Open Game(&O)...\tCtrl+O",    ID_FILE_OPEN
        MENUITEM "Save Game(&S)\tCtrl+S",       ID_FILE_SAVE
        MENUITEM SEPARATOR
        MENUITEM "Exit(&X)",                    ID_APP_EXIT
