        MENUITEM "Undo(&U)\tCtrl+Z",            ID_EDIT_UNDO
    END
    POPUP "Help(&H)"
    BEGIN
        MENUITEM "AboutGame(&A)...",            ID_APP_ABOUT
    END
END

